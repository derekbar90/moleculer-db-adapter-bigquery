{
  "name": "@thesatoshicompany/moleculer-db-adapter-bigquery",
  "version": "1.0.0",
  "description": "SQL adapter with BigQuery for Moleculer DB service",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec npm run build && node examples/index.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "build": "tsc -b",
    "lint": "eslint --ext=.js src test",
    "deps": "npm-check -u",
    "readme": "node node_modules/moleculer-docgen/index.js -t README.md src/index.js",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "microservice",
    "moleculer",
    "orm",
    "database"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moleculerjs/moleculer-db.git"
  },
  "author": "Derek Barrera <derekbarrera@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "moleculer": "^0.12.0 || ^0.13.0 || ^0.14.0",
    "sequelize": "^5.9.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^10",
    "benchmarkify": "^2.1.3",
    "coveralls": "^3.1.0",
    "eslint": "^7.26.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lolex": "^6.0.0",
    "moleculer": "^0.14.13",
    "moleculer-docgen": "^0.3.0",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "npm-check": "^5.9.2",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2",
    "tedious": "^11.0.8",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/services/"
    ]
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^5.6.0",
    "bluebird": "^3.7.2",
    "knex": "^0.95.6"
  }
}
